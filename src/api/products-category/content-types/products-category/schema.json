{
  "kind": "collectionType",
  "collectionName": "products_categories",
  "info": {
    "singularName": "products-category",
    "pluralName": "products-categories",
    "displayName": "Products category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "categoryName": {
      "type": "string",
      "required": true,
      "pluginOptions": {
