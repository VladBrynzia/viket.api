{
  "kind": "collectionType",
  "collectionName": "articles_categories",
  "info": {
    "singularName": "articles-category",
    "pluralName": "articles-categories",
    "displayName": "Articles Category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "categoryId": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "required": true
    },
    "categoryName": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "unique": false
    },
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::art.art",
      "mappedBy": "category"
    },
    "subcategory": {
      "displayName": "subcategory",
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "subcategory.subcategory"
    }
  }
}
